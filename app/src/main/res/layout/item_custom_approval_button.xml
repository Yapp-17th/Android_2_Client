<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>
        <import type="android.view.View" />
        <variable
            name="ChattingViewModel"
            type="com.yapp.sport_planet.presentation.chatting.viewmodel.ChattingActivityViewModel" />
    </data>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <com.google.android.material.button.MaterialButton
            android:id="@+id/bt_custom_approval_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/medium_basic_14sp"
            android:textColor="@color/white"
            android:backgroundTint="@color/white"
            android:layout_gravity="center"
            app:strokeWidth="1dp"
            app:cornerRadius="@dimen/activity_chatting_btn_corner_radius_20dp"
            tools:ignore="MissingConstraints"
            android:stateListAnimator="@null"
            android:letterSpacing="0"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>