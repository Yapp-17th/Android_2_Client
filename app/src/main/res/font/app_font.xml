<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="UnusedAttribute,ResourceCycle">


    <!--regular-->
    <font
        android:font="@font/noto_sans_kr_regular"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/noto_sans_kr_regular"
        app:fontStyle="normal"
        app:fontWeight="400" />
    <!--medium-->
    <font
        android:font="@font/noto_sans_kr_medium"
        android:fontStyle="normal"
        android:fontWeight="500"
        app:font="@font/noto_sans_kr_medium"
        app:fontStyle="normal"
        app:fontWeight="500" />
    <!--bold-->
    <font
        android:font="@font/noto_sans_kr_bold"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/noto_sans_kr_bold"
        app:fontStyle="normal"
        app:fontWeight="700" />


</font-family>